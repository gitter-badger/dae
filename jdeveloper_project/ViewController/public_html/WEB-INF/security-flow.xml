<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="security-flow">
    <default-activity id="__1">login</default-activity>
    <view id="login"></view>
    <router id="router1"></router>
    <task-flow-call id="emp-update-flow">
      <task-flow-reference>
        <document>/WEB-INF/emp-update-flow.xml</document>
        <id>emp-update-flow</id>
      </task-flow-reference>
    </task-flow-call>
    <view id="error"></view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
